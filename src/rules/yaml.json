{
  "no-irregular-whitespace": "off",
  "no-unused-vars": "off",
  "spaced-comment": "off",
  "yaml/block-mapping-colon-indicator-newline": "error",
  "yaml/block-mapping-question-indicator-newline": "error",
  "yaml/block-sequence-hyphen-indicator-newline": "error",
  "yaml/file-extension": "error",
  "yaml/flow-mapping-curly-newline": "error",
  "yaml/flow-mapping-curly-spacing": "off",
  "yaml/flow-sequence-bracket-newline": "error",
  "yaml/flow-sequence-bracket-spacing": "off",
  "yaml/indent": "off",
  "yaml/key-spacing": "off",
  "yaml/no-empty-document": "error",
  "yaml/no-empty-key": "error",
  "yaml/no-empty-mapping-value": "off",
  "yaml/no-empty-sequence-entry": "error",
  "yaml/no-irregular-whitespace": "error",
  "yaml/no-multiple-empty-lines": "error",
  "yaml/no-tab-indent": "error",
  "yaml/no-trailing-zeros": "error",
  "yaml/quotes": "off",
  "yaml/sort-keys": [
    "error",
    {
      "allowLineSeparatedGroups": true,
      "minKeys": 2,
      "order": { "caseSensitive": true, "natural": false, "type": "asc" },
      "pathPattern": ".*"
    }
  ],
  "yaml/spaced-comment": "error",
  "yaml/vue-custom-block/no-parsing-error": "error"
}
